from turtle import Screen, Turtle

screen = Screen()
screen.setup(width = 600, height = 600)
screen.bgcolor("black")
screen.title("My snake game")


erzho = Turtle("square")
erzho.color("white")
erzho.goto(0, 0)

erzho1 = Turtle("Square")
erzho1.color("white")

erzho3 = Turtle("square")
erzho3.color("white")




screen.exitonclick()